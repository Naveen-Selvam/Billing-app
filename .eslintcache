/naveen/Desktop/project1/billing-app/src/components/Auth/Login.js",[],"/home/naveen/Desktop/project1/billing-app/src/components/customer/MainCustomer.js",["152"],"import React, { useEffect } from \"react\";\nimport CustomerItem from \"./CustomerItem\";\nimport CustomerForm from \"./CustomerForm\";\nimport { startGetCustomer } from \"../../action/customeraction\";\nimport { useDispatch, useSelector } from \"react-redux\";\n\nconst MainCustomer = (props) => {\n  const dispatch = useDispatch();\n\n  useEffect(() => {\n    dispatch(startGetCustomer());\n  }, []);\n\n  const customers = useSelector((state) => state.customer);\n\n  return (\n    <div>\n      <div>\n        <CustomerForm />\n      </div>\n      <div>\n        {customers.length === 0 ? (\n          <h2 style={{ textAlign: \"center\", color: \"red\" }}>\n            No Existing Customer\n          </h2>\n        ) : (\n          <CustomerItem customers={customers} />\n        )}\n      </div>\n    </div>\n  );\n};\n\nexport default MainCustomer;\n","/home/naveen/Desktop/project1/billing-app/src/components/customer/CustomerForm.js",[],"/home/naveen/Desktop/project1/billing-app/src/components/customer/CustomerItem.js",[],"/home/naveen/Desktop/project1/billing-app/src/components/product/MainProduct.js",["153"],"import React, { useEffect } from \"react\";\nimport ProductForm from \"./ProductForm\";\nimport ProductItem from \"./ProductItem\";\nimport { startGetProduct } from \"../../action/productaction\";\nimport { useDispatch, useSelector } from \"react-redux\";\n\nconst MainProduct = (props) => {\n  const dispatch = useDispatch();\n\n  useEffect(() => {\n    dispatch(startGetProduct());\n  }, []);\n\n  const products = useSelector((state) => state.product);\n\n  return (\n    <div>\n      <div>\n        <ProductForm />\n      </div>\n      <div>\n        {products.length === 0 ? (\n          <h2 style={{ textAlign: \"center\", color: \"red\" }}>\n            No Existing Products\n          </h2>\n        ) : (\n          <ProductItem products={products} />\n        )}\n      </div>\n    </div>\n  );\n};\n\nexport default MainProduct;\n","/home/naveen/Desktop/project1/billing-app/src/components/product/ProductItem.js",[],"/home/naveen/Desktop/project1/billing-app/src/components/product/ProductForm.js",[],"/home/naveen/Desktop/project1/billing-app/src/components/bill/MainBilling.js",[],"/home/naveen/Desktop/project1/billing-app/src/components/bill/BillingItem.js",["154"],"import React, { useEffect, useState } from \"react\";\nimport {\n  NativeSelect,\n  Grid,\n  Paper,\n  TextField,\n  InputLabel,\n  Button,\n} from \"@material-ui/core\";\nimport { paperStyle6 } from \"../style\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { startGetCustomer } from \"../../action/customeraction\";\nimport { startGetProduct } from \"../../action/productaction\";\nimport { startLineItems, startEmptyLineItems } from \"../../action/lineaction\";\nimport { startPostBill } from \"../../action/billAction\";\nimport GenerateBill from \"./GenerateBill\";\nimport Inovice from \"./Inovice\";\nimport swal from \"sweetalert\";\n\nconst BillingItem = (props) => {\n  const [customer, setCustomer] = useState(\"\");\n  const [product, setProduct] = useState(\"\");\n  const [qty, setQty] = useState(1);\n  const [date, setDate] = useState(new Date());\n  const [toggle, setToggleFunc] = useState(false);\n  const lineItems = useSelector((state) => state.lineItems);\n\n  const dispatch = useDispatch();\n  useEffect(() => {\n    dispatch(startGetCustomer());\n    dispatch(startGetProduct());\n  }, []);\n\n  const customers = useSelector((state) => state.customer);\n  const products = useSelector((state) => state.product);\n\n  const handleChangeCustomer = (e) => {\n    setCustomer(e.target.value);\n  };\n\n  const handleChangeProduct = (e) => {\n    if (e.target.name === \"qty\") {\n      setQty(e.target.value);\n    } else {\n      setProduct(e.target.value);\n    }\n  };\n\n  const handleDate = (e) => {\n    setDate(e.target.value);\n  };\n\n  const handleToggleFunc = () => {\n    setToggleFunc(!toggle);\n  };\n\n  const handleAdd = () => {\n    if (customer.length === 0 || product.length === 0 || date.length === 0) {\n      swal(\"Add your Items\");\n    } else {\n      const data = {\n        product: product,\n        quantity: qty,\n      };\n      dispatch(startLineItems(data));\n      setQty(1);\n      setProduct(\"\");\n    }\n  };\n\n  const handleSubmit = (e) => {\n    e.preventDefault();\n    const formData = {\n      date,\n      customer,\n      lineItems,\n    };\n    if (lineItems.length > 0) {\n      dispatch(startPostBill(formData));\n      handleToggleFunc();\n      setDate(\"\");\n      setCustomer(\"\");\n      setProduct(\"\");\n      setQty(\"\");\n      dispatch(startEmptyLineItems());\n    } else {\n      swal(\"Add your Items before generating the bill\");\n    }\n  };\n\n  return (\n    <div>\n      <div>\n        <Grid>\n          <Paper elevation={20} style={paperStyle6}>\n            <form onSubmit={handleSubmit}>\n              <TextField\n                id=\"date\"\n                label=\"date\"\n                format=\"yyyy-MM-dd\"\n                type=\"date\"\n                value={date}\n                onChange={handleDate}\n                required\n                InputLabelProps={{\n                  shrink: true,\n                }}\n              />\n              <InputLabel>Customer</InputLabel>\n              <NativeSelect\n                className=\"bill\"\n                onChange={handleChangeCustomer}\n                value={customer}\n                required\n              >\n                <option value=\"\" />\n                {customers.map((customer) => {\n                  return (\n                    <option key={customer._id} value={customer._id}>\n                      {customer.name}\n                    </option>\n                  );\n                })}\n              </NativeSelect>\n              <InputLabel>Product</InputLabel>\n              <NativeSelect\n                className=\"bill\"\n                onChange={handleChangeProduct}\n                value={product}\n              >\n                <option value=\"\" />\n                {products.map((product) => {\n                  return (\n                    <option key={product._id} value={product._id}>\n                      {product.name} - â‚¹ {product.price}\n                    </option>\n                  );\n                })}\n              </NativeSelect>\n              <TextField\n                className=\"bill1\"\n                type=\"number\"\n                label=\"Qty\"\n                value={qty}\n                onChange={handleChangeProduct}\n                name=\"qty\"\n                required\n              />\n              <Button\n                type=\"button\"\n                onClick={handleAdd}\n                color=\"primary\"\n                variant=\"contained\"\n              >\n                Add\n              </Button>\n              <br />\n              <Button\n                type=\"submit\"\n                variant=\"contained\"\n                style={{ backgroundColor: \"green\" }}\n              >\n                Generate Bill\n              </Button>\n            </form>\n          </Paper>\n        </Grid>\n      </div>\n      <div>\n        {lineItems?.length === 0 ? (\n          <h2 style={{ textAlign: \"center\", color: \"red\" }}>\n            Add Products for Billing\n          </h2>\n        ) : (\n          <div>\n            <h2>Items in cart</h2>\n            {lineItems?.map((item, i) => {\n              return <GenerateBill key={i} {...item} />;\n            })}\n          </div>\n        )}\n      </div>\n      <div>\n        {toggle && (\n          <Inovice handleToggleFunc={handleToggleFunc} toggle={toggle} />\n        )}\n      </div>\n    </div>\n  );\n};\n\nexport default BillingItem;\n","/home/naveen/Desktop/project1/billing-app/src/components/bill/GenerateBill.js",[],"/home/naveen/Desktop/project1/billing-app/src/components/bill/Inovice.js",["155"],"import React, { useEffect } from \"react\";\nimport Modal from \"react-modal\";\nimport { Button } from \"@material-ui/core\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport { startGetCustomer } from \"../../action/customeraction\";\nimport { startGetProduct } from \"../../action/productaction\";\nimport Table from \"@material-ui/core/Table\";\nimport TableBody from \"@material-ui/core/TableBody\";\nimport TableCell from \"@material-ui/core/TableCell\";\nimport TableContainer from \"@material-ui/core/TableContainer\";\nimport TableHead from \"@material-ui/core/TableHead\";\nimport TableRow from \"@material-ui/core/TableRow\";\n\nconst Inovice = (props) => {\n  const dispatch = useDispatch();\n  const { handleToggleFunc } = props;\n  useEffect(() => {\n    dispatch(startGetCustomer());\n    dispatch(startGetProduct());\n  }, []);\n\n  const products = useSelector((state) => state.product);\n  const customers = useSelector((state) => state.customer);\n  const currentBill = useSelector((state) => state.currentBill);\n  let lineItems = currentBill?.lineItems;\n  let customer = [];\n  let product = [];\n\n  const handleClose = () => {\n    handleToggleFunc();\n  };\n  const billId = localStorage.getItem(\"bill_id\");\n\n  if (Object.keys(currentBill).length > 0) {\n    customer = customers.filter((customer) => {\n      return customer._id === currentBill.customer;\n    });\n  }\n\n  if (lineItems?.length > 0) {\n    lineItems.forEach((item) => {\n      const result = products.filter((product) => {\n        return product._id === item.product;\n      });\n      product.push(result[0]);\n    });\n  }\n\n  return (\n    <div>\n      <Modal isOpen={billId !== \"undefined\"}>\n        <h1>Invoice</h1>\n        <h2>Date - {currentBill?.date?.slice(0, 10)}</h2>\n        <h2>{customer[0]?.name}</h2>\n        <TableContainer>\n          <Table>\n            <TableHead>\n              <TableRow style={{ backgroundColor: \"#404040\" }}>\n                <TableCell>\n                  <h4>Product</h4>\n                </TableCell>\n                <TableCell>\n                  <h4>Price</h4>\n                </TableCell>\n                <TableCell>\n                  <h4>qty</h4>\n                </TableCell>\n                <TableCell>\n                  <h4>subtotal</h4>\n                </TableCell>\n              </TableRow>\n            </TableHead>\n            <TableBody>\n              {product.map((pdt, i) => {\n                return (\n                  <TableRow key={pdt.id}>\n                    <TableCell>{pdt.name}</TableCell>\n                    <TableCell>{pdt.price}</TableCell>\n                    <TableCell>{currentBill?.lineItems[i].quantity}</TableCell>\n                    <TableCell>{currentBill?.lineItems[i].subTotal}</TableCell>\n                  </TableRow>\n                );\n              })}\n            </TableBody>\n          </Table>\n        </TableContainer>\n        <h2> Total Amount = {currentBill.total}</h2>\n        <div style={{ textAlign: \"center\" }}>\n          <Button color=\"secondary\" onClick={handleClose}>\n            close\n          </Button>\n        </div>\n      </Modal>\n    </div>\n  );\n};\n\nexport default Inovice;\n",{"ruleId":"156","replacedBy":"157"},{"ruleId":"158","replacedBy":"159"},{"ruleId":"160","severity":1,"message":"161","line":14,"column":6,"nodeType":"162","endLine":14,"endColumn":8,"suggestions":"163"},{"ruleId":"160","severity":1,"message":"161","line":11,"column":6,"nodeType":"162","endLine":11,"endColumn":8,"suggestions":"164"},{"ruleId":"156","replacedBy":"165"},{"ruleId":"158","replacedBy":"166"},{"ruleId":"160","severity":1,"message":"161","line":12,"column":6,"nodeType":"162","endLine":12,"endColumn":8,"suggestions":"167"},{"ruleId":"160","severity":1,"message":"161","line":12,"column":6,"n