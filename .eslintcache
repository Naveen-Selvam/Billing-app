gin(!login);\n  };\n  const handleLogout = () => {\n    localStorage.clear();\n    setLogin(!login);\n    swal(\"Logged Out Successfully\");\n  };\n  return (\n    <div>\n      <NavbarLink>\n        <div>\n          {!login ? (\n            <h2 style={{ marginRight: \"50rem\" }}>Billing Application</h2>\n          ) : (\n            <img\n              style={{\n                width: \"110px\",\n                height: \"80px\",\n                paddingRight: \"40rem\",\n              }}\n              src={img3}\n            />\n          )}\n        </div>\n\n        {login ? (\n          <div>\n            <Link className=\"link\" to=\"/dashboard\">\n              Dashboard\n            </Link>\n            <Link className=\"link\" to=\"/customer\">\n              Customer\n            </Link>\n            <Link className=\"link\" to=\"/product\">\n              Product\n            </Link>\n            <Link className=\"link\" to=\"/billing\">\n              Billing\n            </Link>\n            <Link className=\"link\" to=\"/profile\">\n              Profile\n            </Link>\n            <Link className=\"link\" onClick={handleLogout} to=\"/\">\n              Logout\n            </Link>\n          </div>\n        ) : (\n          <div>\n            <Link className=\"link\" to=\"/\">\n              Home\n            </Link>\n            <Link className=\"link\" to=\"/register\">\n              Register\n            </Link>\n            <Link className=\"link\" to=\"/login\">\n              Login\n            </Link>\n          </div>\n        )}\n      </NavbarLink>\n      <Route path=\"/\" exact component={Home} />\n      <Route path=\"/register\" exact component={Register} />\n      <Route\n        path=\"/login\"\n        exact\n        render={(props) => <Login {...props} toggleFunc={toggleFunc} />}\n      />\n      {login ? (\n        <div>\n          <Route path=\"/dashboard\" exact component={Dashboard} />\n          <Route path=\"/customer\" exact component={MainCustomer} />\n          <Route path=\"/product\" exact component={MainProduct} />\n          <Route path=\"/billing\" exact component={MainBilling} />\n          <Route path=\"/profile\" exact component={Profile} />\n          <Route path=\"/customer/:id\" exact component={CustomerView} />\n        </div>\n      ) : (\n        <div>\n          <Redirect to=\"/\" />\n        </div>\n      )}\n    </div>\n  );\n};\n\nexport default Navbar;\n","/home/naveen/Desktop/project1/billing-app/src/reducer/customerReducer.js",[],"/home/naveen/Desktop/project1/billing-app/src/reducer/productReducer.js",[],"/home/naveen/Desktop/project1/billing-app/src/reducer/profileReducer.js",[],"/home/naveen/Desktop/project1/billing-app/src/config/axios.js",[],"/home/naveen/Desktop/project1/billing-app/src/components/Dahboard.js",["158","159"],"import React, { useEffect, useState } from \"react\";\nimport { startGetCustomer } from \"../action/customeraction\";\nimport { startGetProduct } from \"../action/productaction\";\nimport { startGetBill } from \"../action/billAction\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { Grid, Paper } from \"@material-ui/core\";\nimport { paperStyle3, paperStyle7 } from \"../components/style\";\nimport EachCustomerBill from \"../components/customer/EachCustomerBill\";\n\nconst Dahboard = (props) => {\n  const dispatch = useDispatch();\n  let sum = 0;\n\n  useEffect(() => {\n    dispatch(startGetCustomer());\n    dispatch(startGetProduct());\n    dispatch(startGetBill());\n  }, []);\n\n  const customer = useSelector((state) => state.customer);\n  const product = useSelector((state) => state.product);\n  const bills = useSelector((state) => state.bill);\n\n  let last5cust = customer?.slice(customer.length - 5);\n  let last5prod = product?.slice(product.length - 5);\n  let last5bill = bills?.slice(bills.length - 5);\n  let customerName = [];\n\n  if (bills.length > 0) {\n    bills.forEach((bill, i) => {\n      sum += bill.total;\n    });\n  }\n\n  if (last5bill.length > 0) {\n    last5bill.forEach((bill) => {\n      const result = customer?.filter((cust) => {\n        return cust._id === bill.customer;\n      });\n      customerName.push(result[0]);\n    });\n  }\n\n  return (\n    <div>\n      <h1 style={{ textAlign: \"center\", marginTop: \"1rem\" }}>Dashboard</h1>\n      <div style={{ display: \"flex\", justifyContent: \"center\" }}>\n        <Grid style={{ padding: \"2rem\" }}>\n          <Paper elevation={10} style={paperStyle3}>\n            <h1>Total Customers</h1>\n            <h3 className=\"number\">{customer.length}</h3>\n          </Paper>\n        </Grid>\n\n        <Grid style={{ padding: \"2rem\" }}>\n          <Paper elevation={10} style={paperStyle3}>\n            <h1>Total Products</h1>\n            <h3 className=\"number\">{product.length}</h3>\n          </Paper>\n        </Grid>\n\n        <Grid style={{ padding: \"2rem\" }}>\n          <Paper elevation={10} style={paperStyle3}>\n            <h1>Total Bills</h1>\n            <h3 className=\"number\">{bills.length}</h3>\n          </Paper>\n        </Grid>\n      </div>\n\n      <div style={{ display: \"flex\", justifyContent: \"center\" }}>\n        <Grid style={{ padding: \"2rem\" }}>\n          <Paper elevation={10} style={paperStyle3}>\n            <h1>Total Sales</h1>\n            <h3 className=\"number\">{sum}</h3>\n          </Paper>\n        </Grid>\n      </div>\n\n      <div>\n        {last5cust.length > 0 && <h2>Last 5 added customers</h2>}\n        <div style={{ display: \"flex\" }}>\n          {last5cust?.map((cust) => {\n            return (\n              <Grid style={{ padding: \"0.5rem\", backgroundColor: \"#e6ecff\" }}>\n                <Paper elevation={10} style={paperStyle7} key={cust._id}>\n                  <h3>Name - {cust.name}</h3>\n                  <h3>Mobile - {cust.mobile}</h3>\n                  <h3>mail-{cust.email}</h3>\n                </Paper>\n              </Grid>\n            );\n          })}\n        </div>\n\n        {last5prod.length > 0 && <h2>Last 5 added products</h2>}\n        <div style={{ display: \"flex\" }}>\n          {last5prod?.map((prod) => {\n            return (\n              <Grid style={{ padding: \"0.5rem\", backgroundColor: \"#ebfafa\" }}>\n                <Paper elevation={10} style={paperStyle7} key={prod._id}>\n                  <h3>Name - {prod.name}</h3>\n                  <h3>Price - {prod.price}</h3>\n                </Paper>\n              </Grid>\n            );\n          })}\n        </div>\n      </div>\n\n      <div>\n        {last5bill.length > 0 && <h2>Last 5 generated Bills</h2>}\n        <div className=\"customerViewTable\">\n          {last5bill?.map((bill, i) => {\n            return (\n              <div key={bill._id}>\n                <div>\n                  <h3>Name - {customerName[i]?.name}</h3>\n                  <EachCustomerBill customer={bill} />\n                </div>\n              </div>\n            );\n          })}\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default Dahboard;\n","/home/naveen/Desktop/project1/billing-app/src/components/Profile.js",["160"],"import React, { useEffect } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { startGetUser } from \"../action/userAction\";\nimport img1 from \"../img/img_avatar.png\";\n\nconst Profile = (props) => {\n  const dispatch = useDispatch();\n\n  useEffect(() => {\n    dispatch(startGetUser());\n  }, []);\n\n  const result = useSelector((state) => state.profile);\n\n  return (\n    <div>\n      <h1 style={{ textAlign: \"center\" }}>Profile</h1>\n      <div className=\"card\">\n        <img\n          src={img1}\n          alt=\"Avatar\"\n          style={{ width: \"50%\", marginLeft: \"5.5rem\" }}\n        ></img>\n        <div className=\"container\">\n          <div style={{ textAlign: \"center\" }}>\n            <h3>Name - {result?.username}</h3>\n            <h3>E-mail - {resu