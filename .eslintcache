t bills = useSelector((state) => state.bill);\n\n  if (bills.length > 0) {\n    bills.forEach((bill, i) => {\n      sum += bill.total;\n    });\n  }\n  console.log(sum);\n\n  return (\n    <div>\n      <h1 style={{ textAlign: \"center\" }}>Dashboard</h1>\n      <div style={{ display: \"flex\", justifyContent: \"center\" }}>\n        <Grid style={{ padding: \"2rem\" }}>\n          <Paper elevation={10} style={paperStyle3}>\n            <h1>Total Customers</h1>\n            <h3 className=\"number\">{customer.length}</h3>\n          </Paper>\n        </Grid>\n        <Grid style={{ padding: \"2rem\" }}>\n          <Paper elevation={10} style={paperStyle3}>\n            <h1>Total Products</h1>\n            <h3 className=\"number\">{product.length}</h3>\n          </Paper>\n        </Grid>\n        <Grid style={{ padding: \"2rem\" }}>\n          <Paper elevation={10} style={paperStyle3}>\n            <h1>Total Bills</h1>\n            <h3 className=\"number\">{bills.length}</h3>\n          </Paper>\n        </Grid>\n      </div>\n      <div style={{ display: \"flex\", justifyContent: \"center\" }}>\n        <Grid style={{ padding: \"2rem\" }}>\n          <Paper elevation={10} style={paperStyle3}>\n            <h1>Total Sales</h1>\n            <h3 className=\"number\">{sum}</h3>\n          </Paper>\n        </Grid>\n      </div>\n    </div>\n  );\n};\n\nexport default Dahboard;\n","/home/naveen/Desktop/project1/billing-app/src/components/Profile.js",["156"],"import React, { useEffect } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { startGetUser } from \"../action/userAction\";\nimport img1 from \"../img/img_avatar.png\";\n\nconst Profile = (props) => {\n  const dispatch = useDispatch();\n\n  useEffect(() => {\n    dispatch(startGetUser());\n  }, []);\n\n  const result = useSelector((state) => state.profile);\n\n  return (\n    <div>\n      <h1 style={{ textAlign: \"center\" }}>Profile</h1>\n      <div className=\"card\">\n        <img\n          src={img1}\n          alt=\"Avatar\"\n          style={{ width: \"50%\", marginLeft: \"5.5rem\" }}\n        ></img>\n        <div className=\"container\">\n          <div style={{ textAlign: \"center\" }}>\n            <h3>Name - {result?.username}</h3>\n            <h3>E-mail - {result?.email}</h3>\n            <h3>Businessname - {result?.businessName}</h3>\n            <h3>Address - {result?.address}</h3>\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default Profile;\n","/home/naveen/Desktop/project1/billing-app/src/action/userAction.js",[],"/home/naveen/Desktop/project1/billing-app/src/components/style.js",[],"/home/naveen/Desktop/project1/billing-app/src/action/productaction.js",[],"/home/naveen/Desktop/project1/billing-app/src/reducer/billReducer.js",[],"/home/naveen/Desktop/project1/billing-app/src/action/billAction.js",[],"/home/naveen/Desktop/project1/billing-app/src/reducer/lineItemReducer.js",[],"/home/naveen/Desktop/project1/billing-app/src/action/lineaction.js",[],"/home/naveen/Desktop/project1/billing-app/src/reducer/currentBillReducer.js",[],"/home/naveen/Desktop/project1/billing-app/src/action/currentBillAction.js",[],"/home/naveen/Desktop/project1/billing-app/src/components/Auth/Home.js",[],"/home/naveen/Desktop/project1/billing-app/src/components/Auth/Register.js",[],"/home/naveen/Desktop/project1/billing-app/src/components/Auth/Login.js",[],"/home/naveen/Desktop/project1/billing-app/src/components/customer/MainCustomer.js",["157"],"import React, { useEffect } from \"react\";\nimport CustomerItem from \"./CustomerItem\";\nimport CustomerForm from \"./CustomerForm\";\nimport { startGetCustomer } from \"../../action/customeraction\";\nimport { useDispatch, useSelector } from \"react-redux\";\n\nconst MainCustomer = (props) => {\n  const dispatch = useDispatch();\n\n  useEffect(() => {\n    dispatch(startGetCustomer());\n  }, []);\n\n  const customers = useSelector((state) => state.customer);\n\n  return (\n    <div>\n      <div>\n        <CustomerForm />\n      </div>\n      <div>\n        {customers.length === 0 ? (\n          <h2 style={{ textAlign: \"center\", color: \"red\" }}>\n            No Existing Customer\n          </h2>\n        ) : (\n          <CustomerItem customers={customers} />\n        )}\n      </div>\n    </div>\n  );\n};\n\nexport default MainCustomer;\n","/home/naveen/Desktop/project1/billing-app/src/components/customer/CustomerForm.js",[],"/home/naveen/Desktop/project1/billing-app/src/components/customer/CustomerItem.js",[],"/home/naveen/Desktop/project1/billing-app/src/components/product/MainProduct.js",["158"],"import React, { useEffect } from \"react\";\nimport ProductForm from \"./ProductForm\";\nimport ProductItem from \"./ProductItem\";\nimport { startGetProduct } from \"../../action/productaction\";\nimport { useDispatch, useSelector } from \"react-redux\";\n\nconst MainProduct = (props) => {\n  const dispatch = useDispatch();\n\n  useEffect(() => {\n    dispatch(startGetProduct());\n  }, []);\n\n  const products = useSelector((state) => state.product);\n\n  return (\n    <div>\n      <div>\n        <ProductForm />\n      </div>\n      <div>\n        {products.length === 0 ? (\n          <h2 style={{ textAlign: \"center\", color: \"red\" }}>\n            No Existing Products\n          </h2>\n        ) : (\n          <ProductItem products={products} />\n        )}\n      </div>\n    </div>\n  );\n};\n\nexport default MainProduct;\n","/home/naveen/Desktop/project1/billing-app/src/components/product/ProductItem.js",[],"/home/naveen/Desktop/project1/billing-app/src/components/product/ProductForm.js",[],"/home/naveen/Desktop/project1/billing-app/src/components/bill/MainBilling.js",[],"/home/naveen/Desktop/project1/billing-app/src/components/bill/BillingItem.js",["159"],"import React, { useEffect, useState } from \"react\";\nimport {\n  NativeSelect,\n  Grid,\n  Paper,\n  TextField,\n  InputLabel,\n  Button,\n} from \"@material-ui/core\";\nimport { paperStyle6 } from \"../style\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { startGetCustomer } from \"../../action/customeraction\";\nimport { startGetProduct } from \"../../action/productaction\";\nimport { startLineItems, startEmptyLineItems } from \"../../action/lineaction\";\nimport { startPostBill } from \"../../action/billAction\";\nimport GenerateBill from \"./GenerateBill\";\nimport Inovice from \"./Inovice\";\nimport swal from \"sweetalert\";\n\nconst BillingItem = (props) => {\n  const [customer, setCustomer] = useState(\"\");\n  const [product, setProduct] = useState(\"\");\n  const [qty, setQty] = useState(1);\n  const [date, setDate] = useState(new Date());\n  console.log(date);\n\n  const [toggle, setToggleFunc] = useState(false);\n  const lineItems = useSelector((state) => state.lineItems);\n\n  const dispatch = useDispatch();\n  useEffect(() => {\n    dispatch(startGetCustomer());\n    dispatch(startGetProduct());\n  }, []);\n\n  const customers = useSelector((state) => state.customer);\n  const products = useSelector((state) => state.product);\n\n  const handleChangeCustomer = (e) => {\n    setCustomer(e.target.value);\n  };\n\n  const handleChangeProduct = (e) => {\n    if (e.target.name === \"qty\") {\n      setQty(e.target.value);\n    } else {\n      setProduct(e.target.value);\n    }\n  };\n\n  const handleDate = (e) => {\n    setDate(e.target.value);\n  };\n\n  const handleToggleFunc = () => {\n    setToggleFunc(!toggle);\n  };\n\n  const handleAdd = () => {\n    if (customer.length === 0 || product.length === 0 || date.length === 0) {\n      swal(\"Add your Items\");\n    } else {\n      const data = {\n        product: product,\n        quantity: qty,\n      };\n      dispatch(startLineItems(data));\n      setQty(1);\n      setProduct(\"\");\n    }\n  };\n\n  const handleSubmit = (e) => {\n    e.preventDefault();\n    const formData = {\n      date,\n      customer,\n      lineItems,\n    };\n    if (lineItems.length > 0) {\n      dispatch(startPostBill(formData));\n      handleToggleFunc();\n      setCustomer(\"\");\n      setProduct(\"\");\n      setQty(\"\");\n      dispatch(startEmptyLineItems());\n    } else {\n      swal(\"Add your Items before generating the bill\");\n    }\n  };\n\n  return (\n    <div>\n      <div>\n        <Grid>\n          <Paper elevation={20} style={paperStyle6}>\n            <form onSubmit={handleSubmit}>\n              <TextField\n                id=\"date\"\n                label=\"date\"\n                format=\"yyyy-MM-dd\"\n                type=\"date\"\n                value={date}\n                onChange={handleDate}\n                required\n                InputLabelProps={{\n                  shrink: true,\n                }}\n              />\n              <InputLabel>Customer</InputLabel>\n              <NativeSelect\n                className=\"bill\"\n                onChange={handleChangeCustomer}\n                value={customer}\n                required\n              >\n                <option value=\"\" />\n                {customers.map((customer) => {\n                  return (\n                    <option key={customer._id} value={customer._id}>\n                      {customer.name}\n                    </option>\n                  );\n                })}\n              </NativeSelect>\n              <InputLabel>Product</InputLabel>\n              <NativeSelect\n                className=\"bill\"\n                onChange={handleChangeProduct}\n                value={product}\n              >\n                <option value=\"\" />\n                {products.map((product) => {\n                  return (\n                    <option key={product._id} value={product._id}>\n                      {product.name} - â‚¹ {product.price}\n                    </option>\n                  );\n                })}\n              </NativeSelect>\n              <TextField\n                className=\"bill1\"\n               